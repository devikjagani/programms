<!DOCTYPE html>
<html>
<head>
    <title>My Portfolio</title>
    <style>
        body { font-family: Arial; background: #f4f4f4; padding: 20px; }
        h1 { color: #333; }
        .project { margin-bottom: 15px; padding: 10px; background: #fff; border-radius: 8px; }
    </style>
</head>
<body>
    <h1>Hi, I'm <%= name %></h1>
    <p>I'm a full stack developer and cybersecurity learner.</p>

    <h2>Projects</h2>
    <% projects.forEach(function(project) { %>
        <div class="project"><strong><%= project %></strong></div>
    <% }); %>
</body>
</html>
